{
  "name": "typology-processor",
  "version": "0.1.0",
  "description": "typology-processor",
  "module": "src/index.ts",
  "type": "module",
  "scripts": {
    "dev": "nodemon -r dotenv/config",
    "buildOld": "tsc --project tsconfig.json",
    "buildbun": "bun build --target=bun ./src/index.ts --outfile=build/index.js",
    "start": "bun ./src/index.ts",
    "startPreBuilt": "bun build/index.js",
    "startOld": "node -r dotenv/config build/index.js",
    "cleanup": "rm -rf build dist template jest.config.js jest.config.js.map node_modules",
    "fix": "bun run fix:prettier && bun run fix:eslint",
    "fix:eslint": "eslint --fix \"**/*.ts\"",
    "fix:prettier": "prettier --write \"**/*.ts\"",
    "lint": "bun run lint:eslint && bun run lint:prettier",
    "lint:eslint": "eslint \"**/*.ts\"",
    "lint:prettier": "prettier --check \"**/*.ts\"",
    "test": "jest --config=jest.config.ts --passWithNoTests",
    "test:watch": "jest --config=jest.config.ts --watch"
  },
  "keywords": [
    "mojaloop",
    "actio",
    "frm"
  ],
  "contributors": [
    {
      "name": "Johan Foley"
    },
    {
      "name": "Joey GÃ¶ksu"
    }
  ],
  "license": "ISC",
  "dependencies": {
    "@frmscoe/frms-coe-lib": "^1.2.4",
    "@frmscoe/frms-coe-startup-lib": "^2.0.0",
    "dotenv": "^16.0.0",
    "lodash": "^4.17.21",
    "log4js": "^6.4.0",
    "tslib": "^2.6.0"
  },
  "devDependencies": {
    "@types/arangodb": "^3.5.11",
    "@types/axios": "^0.14.0",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.172",
    "@types/log4js": "^2.3.5",
    "@types/nodemon": "^1.19.1",
    "@types/prettier": "^2.3.2",
    "@types/rimraf": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.59.0",
    "bun-types": "^1.0.2",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "8.0.0",
    "ioredis-mock": "^8.8.1",
    "jest": "^29.2.2",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.20",
    "prettier": "^3.0.0",
    "rimraf": "^3.0.2",
    "tplant": "^3.1.0",
    "ts-jest": "^29.0.3",
    "typescript": "^5.0.4"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
